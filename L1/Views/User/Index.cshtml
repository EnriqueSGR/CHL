@model List<L1.Models.TableViewModels.UserTableViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Usuarios</h2>

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>#</th>
                <th>Email</th>
                <th>Edad</th>
                <th></th>
            </tr>

            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.id</td>
                    <td>@oElemento.email</td>
                    <td>@oElemento.edad</td>
                    
                </tr>
            }
        </table>
    </div>
</div>

